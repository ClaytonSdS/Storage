<ItemConfirm>
    on_release: root.set_icon(check)

    CheckboxLeftWidget:
        id: check
        group: "check"

<Fornecedores>
    recycle_view: recycle_view
    items_box: items_box
    orientation: "vertical"
    size_hint_y: None
    height: dp(400)
    spacing: 10

    MDBoxLayout:
        orientation: 'vertical'
        md_bg_color: 1,0,0,0
        pos_hint: {"center_x":0.5, "center_y": 0.5}
        size: self.parent.size
        GridLayout:
            rows: 2
            cols:1
            spacing: 10

            # HEADER SUPERIOR COM NOMES DAS TABELAS E BARRA DE BUSCA
            MDBoxLayout:
                orientation: "vertical"
                size_hint_y: 0.25
                MDBoxLayout:
                    size_hint_y: 0.2
                    MDBoxLayout:
                        size_hint_x:0.8
                        orientation: "horizontal"
                        Label:
                            text: "ID"
                            halign: "center"
                            valign: "middle"
                            font_size: 15
                            markup: True
                            color: get_color_from_hex('#242425')
                            canvas.before:
                                Color:
                                    rgba: get_color_from_hex('#676767')
                                Line:
                                    width: 0.8
                                    rectangle: self.x, self.y, self.width, self.height

                    MDBoxLayout:
                        orientation: "horizontal"
                        size_hint_x: 1.8
                        Label:
                            text: "FORNECEDOR"
                            halign: "center"
                            valign: "middle"
                            font_size: 15
                            markup: True
                            color: get_color_from_hex('#242425')
                            canvas.before:
                                Color:
                                    rgba: get_color_from_hex('#676767')
                                Line:
                                    width: 0.8
                                    rectangle: self.x, self.y, self.width, self.height

                    MDBoxLayout:
                        orientation: "horizontal"
                        Label:
                            text: "-"
                            halign: "center"
                            valign: "middle"
                            font_size: 15
                            markup: True
                            color: get_color_from_hex('#242425')
                            canvas.before:
                                Color:
                                    rgba: get_color_from_hex('#676767')
                                Line:
                                    width: 0.8
                                    rectangle: self.x, self.y, self.width, self.height
                MDBoxLayout:
                    id: search_clientes_box
                    md_bg_color: 0,0,0,0
                    size_hint: (1,0.2)
                    InputCapsLock:
                        id: search_bar_fornecedores
                        size_hint: 2.61, 1
                        height: "30dp"
                        pos_hint: {"center_x": .5, "center_y": .5}
                        canvas.before:
                            Color:
                                rgba: get_color_from_hex('#676767')
                            Line:
                                width: 0.3
                                rectangle: self.x, self.y, self.width, self.height
                    MDBoxLayout:
                        md_bg_color: 1,0,0,0
                        width: "45dp"
                        pos_hint: {"center_x": .5, "center_y": .5}
                        canvas.before:
                            Color:
                                rgba: get_color_from_hex('#3b3b3b')
                            Line:
                                width: 0.3
                                rectangle: self.x, self.y, self.width, self.height
                        MDIconButton:
                            on_release: root.search_fornecedor(root.ids.search_bar_fornecedores.text)
                            icon: "account-search"
                            size_hint_y: 1
                            size_hint_x: 1
                            pos_hint: {"center_x": .5, "center_y": .5}

            MDBoxLayout:
                orientation: 'vertical'
                md_bg_color: 1,0,0,0
                size_hint: (1,1)
                spacing: 5

                RecycleView:
                    id: recycle_view
                    size_hint: 1, 0.9
                    viewclass: "Fornecedores_"
                    RecycleBoxLayout:
                        id: items_box
                        orientation: "vertical"
                        default_size_hint: 1, None
                        size_hint: 1, None
                        height: self.minimum_height





