WindowManager:
    transition: FadeTransition()
    Inicio:
	    name: "Inicio"
	Sell:
	    name: "Sell"
	Storage:
	    name: "Storage"
	IA:
	    name: "IA"

#///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
<Inicio>
    MDGridLayout:
        cols: 2
        rows: 1
        MDBoxLayout:
            id: menu
            orientation: "vertical"
            size_hint_x: None
            width: int(0.185*self.parent.width)
            md_bg_color: get_color_from_hex("#242425")
            spacing: 2
            Menu_Botao:
                size_hint_y: None
                height: 70
                orientation: "horizontal"
                Image:
                    source: 'logo_extensa.png'
                    size_hint: 0.7,0.7
                    pos_hint: {'center_x': .5,'center_y': .5}
                    allow_stretch: False
                    allow_ratio: False
                    center_x: self.parent.center_x
                    center_y: self.parent.center_y

            # BOTÃO INICIO
            Menu_Botao:
                size_hint_y: None
                height: 70
                orientation: "horizontal"
                spacing: 0
                on_release: root.manager.current = "Inicio"
                MDIconButton:
                    icon: "home"
                    size_hint: (0.5,0.5)
                    pos_hint: {"center_x":0.5, "center_y":0.5}
                    theme_icon_color: "Custom"
                    icon_color: get_color_from_hex('#FFFFFF')
                MDLabel:
                    markup: True
                    size_hint: (1,1)
                    color: get_color_from_hex('#FFFFFF')
                    text: "[b]Inicio[/b]"
                    halign: "center"
                    valign: "middle"
                    font_size: 17
                MDIconButton:
                    opacity: 0
                    icon: "chevron-down"
                    size_hint: (0.5,0.5)
                    pos_hint: {"center_x":0.5, "center_y":0.5}
                    theme_icon_color: "Custom"
                    icon_color: get_color_from_hex('#FFFFFF')
            # BOTAO VENDER
            Menu_Botao:
                size_hint_y: None
                height: 70
                orientation: "horizontal"
                spacing: 0
                on_release: root.manager.current = "Sell"
                MDIconButton:
                    icon: "cart-outline"
                    size_hint: (0.5,0.5)
                    pos_hint: {"center_x":0.5, "center_y":0.5}
                    theme_icon_color: "Custom"
                    icon_color: get_color_from_hex('#FFFFFF')
                MDLabel:
                    markup: True
                    size_hint: (1,1)
                    color: get_color_from_hex('#FFFFFF')
                    text: "[b]Vender[/b]"
                    halign: "center"
                    valign: "middle"
                    font_size: 17
                MDIconButton:
                    opacity: 0
                    icon: "chevron-down"
                    size_hint: (0.5,0.5)
                    pos_hint: {"center_x":0.5, "center_y":0.5}
                    theme_icon_color: "Custom"
                    icon_color: get_color_from_hex('#FFFFFF')

            # BOTÃO ESTOQUE
            Menu_Botao:
                size_hint_y: None
                height: 70
                orientation: "horizontal"
                spacing: 0
                on_release: root.manager.current = "Storage"
                MDIconButton:
                    icon: "store"
                    size_hint: (0.5,0.5)
                    pos_hint: {"center_x":0.5, "center_y":0.5}
                    theme_icon_color: "Custom"
                    icon_color: get_color_from_hex('#FFFFFF')
                MDLabel:
                    markup: True
                    size_hint: (1,1)
                    color: get_color_from_hex('#FFFFFF')
                    text: "[b]Estoque[/b]"
                    halign: "center"
                    valign: "middle"
                    font_size: 17
                MDIconButton:
                    opacity: 0
                    icon: "chevron-down"
                    size_hint: (0.5,0.5)
                    pos_hint: {"center_x":0.5, "center_y":0.5}
                    theme_icon_color: "Custom"
                    icon_color: get_color_from_hex('#FFFFFF')
            # BOTÃO ReVision
            Menu_Botao:
                size_hint_y: None
                height: 70
                orientation: "horizontal"
                spacing: 0
                on_release: root.manager.current = "IA"
                MDIconButton:
                    icon: "robot-happy"
                    size_hint: (0.5,0.5)
                    pos_hint: {"center_x":0.5, "center_y":0.5}
                    theme_icon_color: "Custom"
                    icon_color: get_color_from_hex('#FFFFFF')
                MDLabel:
                    markup: True
                    size_hint: (1,1)
                    color: get_color_from_hex('#FFFFFF')
                    text: "[b]ReVision[/b]"
                    halign: "center"
                    valign: "middle"
                    font_size: 17
                MDIconButton:
                    opacity: 0
                    icon: "chevron-down"
                    size_hint: (0.5,0.5)
                    pos_hint: {"center_x":0.5, "center_y":0.5}
                    theme_icon_color: "Custom"
                    icon_color: get_color_from_hex('#FFFFFF')
            # BOTÃO CONFIGURACOES
            Menu_Botao:
                elevation: 10
                size_hint_y: None
                height: 70
                orientation: "horizontal"
                spacing: 0
                on_release: root.manager.current = "Setts"
                MDIconButton:
                    icon: "wrench"
                    size_hint: (0.5,0.5)
                    pos_hint: {"center_x":0.5, "center_y":0.5}
                    theme_icon_color: "Custom"
                    icon_color: get_color_from_hex('#FFFFFF')
                MDLabel:
                    markup: True
                    size_hint: (1,1)
                    color: get_color_from_hex('#FFFFFF')
                    text: "[b]Configurações[/b]"
                    halign: "center"
                    valign: "middle"
                    font_size: 17
                MDIconButton:
                    opacity: 0
                    icon: "chevron-down"
                    size_hint: (0.5,0.5)
                    pos_hint: {"center_x":0.5, "center_y":0.5}
                    theme_icon_color: "Custom"
                    icon_color: get_color_from_hex('#FFFFFF')
            Menu_Botao:
                size_hint: (1,3)

        #/////////////////////////////////////////////////////////////////////////////////////////////////////////////////
        # AREA LIVRE + BARRA DE MENU
        MDBoxLayout:
            orientation: 'vertical'
            pos_hint: 0.5, 0.5
            md_bg_color: get_color_from_hex('#ffffff')
            MDLabel:
                text: "Inicio"
                halign: "center"

