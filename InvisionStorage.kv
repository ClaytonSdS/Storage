<Menu>
    orientation: "vertical"
    size_hint_y: None
    height: self.parent.height
    MDBoxLayout:
        orientation: 'vertical'
        pos_hint: {"center_x":0.5, "center_y": 0.5}
        size: self.parent.size
        GridLayout:
            rows: 8
            cols:1
            spacing: 2
            Menu_Botao:
                size_hint_y: None
                height: 70
                orientation: "horizontal"
                Image:
                    source: 'icon.png'
                    size_hint: 0.5,0.5
                    pos_hint: {'center_x': .5,'center_y': .5}
                    allow_stretch: False
                    allow_ratio: False
                    center_x: self.parent.center_x
                    center_y: self.parent.center_y
            # INICIO
            Menu_Botao:
                orientation: "horizontal"
                ButtonTip:
                    icon: "view-dashboard"
                    icon_size: "35sp"
                    size_hint: (0.5,1)
                    pos_hint: {"center_x":0.5, "center_y":0.5}
                    theme_icon_color: "Custom"
                    icon_color: get_color_from_hex('#242425')
                    on_release: root.parent.parent.parent.manager.current = "Inicio"; self.md_bg_color = get_color_from_hex("#eeeeee")

            # VENDER
            Menu_Botao:
                orientation: "horizontal"
                spacing: 0

                ButtonTip:
                    icon: "cart-outline"
                    icon_size: "35sp"
                    size_hint: (0.5,1)
                    pos_hint: {"center_x":0.5, "center_y":0.5}
                    theme_icon_color: "Custom"
                    icon_color: get_color_from_hex('#242425')
                    on_release: root.parent.parent.parent.manager.current = "Sell"; self.md_bg_color = get_color_from_hex("#eeeeee")

            # ESTOQUE
            Menu_Botao:
                orientation: "horizontal"
                spacing: 0
                ButtonTip:
                    icon: "package-variant"
                    icon_size: "35sp"
                    size_hint: (0.5,1)
                    pos_hint: {"center_x":0.5, "center_y":0.5}
                    theme_icon_color: "Custom"
                    icon_color: get_color_from_hex('#242425')
                    on_release: root.parent.parent.parent.manager.current = "Storage"; self.md_bg_color = get_color_from_hex("#eeeeee")

            # CLIENTES
            Menu_Botao:
                orientation: "horizontal"
                spacing: 0
                ButtonTip:
                    icon: "account-group"
                    icon_size: "35sp"
                    size_hint: (0.5,1)
                    pos_hint: {"center_x":0.5, "center_y":0.5}
                    theme_icon_color: "Custom"
                    icon_color: get_color_from_hex('#242425')
                    on_release: root.parent.parent.parent.manager.current = "Clientes"; self.md_bg_color = get_color_from_hex("#eeeeee")

            # REVISION
            Menu_Botao:
                size_hint_x: 0.5
                orientation: "horizontal"
                spacing: 0

                ButtonTip:
                    icon: "robot-happy"
